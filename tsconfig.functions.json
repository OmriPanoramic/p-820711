{
    "compilerOptions": {
      "target": "ES2020",
      "module": "ESNext",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "jsx": "react-jsx",
      "isolatedModules": true,
      "strict": false,
      "skipLibCheck": true,
      "noImplicitAny": false,
      "paths": {
        "@/*": ["./src/*"]
      }
    },
    "include": ["netlify/functions/**/*"],
  }